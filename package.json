{
    "name": "lb-stanza-vscode",
    "displayName": "L.B. Stanza",
    "description": "Stanza langugage support in VS Code",
    "version": "0.1.0",
    "repository": {
        "url": "https://github.com/stanza-tools/lb-stanza-vscode"
    },
    "engines": {
        "vscode": "^1.68.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "stanza",
            "aliases": ["L.B. Stanza", "stanza"],
            "extensions": [".stanza"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "stanza",
            "scopeName": "source.stanza",
            "path": "./syntaxes/stanza.tmLanguage.json"
        }]
    }
}